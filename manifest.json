{
  "name": "Zendesk Guide Transfer",
  "version": "0.0.1",
  "description": "Transfer a page to Zendesk Guide",
  "permissions": [
    "storage",
    "https://*.zendesk.com/"
  ],
  "manifest_version": 2,
  "browser_action": {
    "default_icon": {
      "32": "image/icon128.png"
    },
    "default_popup": "popup.html"
  },
  "icons" : {
    "128": "image/icon128.png"
  },
  "options_ui": {
    "page": "options.html",
    "chrome_style": true
  }
}